<div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-sm-4">
                    <h2>Profile</h2>
                    <ol class="breadcrumb">
                        <li>
                        <% # TODO use route for Dashboard link%>
                            <a href="index.html">Dashboard</a>
                        </li>
                        <li class="active">
                            <strong>Profile Details</strong>
                        </li>
                    </ol>
                </div>
</div>
<div class="col-lg-9">
  <div class="wrapper wrapper-content animated fadeInUp">
    <div class="ibox">
      <div class="ibox content white-bg">
        <div class="row">
          <div class="col-lg-12">
            <div class="m-b-md">
                <a href="#" class="btn btn-white btn-xs pull-right">Edit Profile</a>
                <h2><%= current_user.first_name + " " + current_user.last_name%></h2>
            </div>
                    <dl class="dl-horizontal">
                    <dt>Status:</dt> <dd><span class="label label-primary">Active</span></dd>
                    </dl>
                    <!-- <div></div> -->
                    <dl class="dl-horizontal">
                    <dt>Company:</dt> <dd><span><%= current_user.business_name%></span></dd>
                    </dl>
                    <dl class="dl-horizontal">
                    <dt>Registration Date:</dt> <dd><span><%= current_user.created_at%></span></dd>
                    </dl>
                    <dl class="dl-horizontal">
                    <dt>Phone:</dt> <dd><span><%= current_user.phone_number%></span></dd>
                    </dl>
                    <dl class="dl-horizontal">
                    <dt>Mobile:</dt> <dd><span><%= current_user.cell_number%></span></dd>
                    </dl>
                    <dl class="dl-horizontal">
                    <dt>Account #:</dt> <dd><span><%= current_user.account_number%></span></dd>
                    </dl>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
