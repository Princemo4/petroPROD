<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Time</th>
                                <th>Status</th>
                              <!--  <th>Action</th> -->
                                <th>87 RFG E10</th>
                                <th>89 RFG E10</th>
                                <th>91 RFG E10</th>
                                <th>93 RFG E10</th>
                                <th>Low Sodium Diesel</th>
                                <th>Dyed ULS Diesel</th>
                                <th>Undyed ULS Diesel</th>
                                <th>Jet Fuel</th>
                                <th>Heating Oil</th>
                                <th>Kerosene</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% x = 0 %>
                            <% @fuel_prices.each do |f| %>
                            <tr class="gradeX">
                                <!-- <td class="mycell col-md-2"><%= time_ago_in_words(f.created_at) %> ago</td>
                                <td class="mycell col-md-1">
                                    <span class="label label-success">Not set</span>
                                </td> -->
                                <td class="mycell col-md-2"><%= time_ago_in_words(f.created_at) %> ago</td>
                               <% if x < 1 %>
                                  <td class="mycell col-md-1">
                                    <%= link_to edit_supplier_fuel_price_path(f), remote: true do %>
                                    <button type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#newFuelPrice">
                                        edit
                                    </button>
                                    <%end%>
                                  </td>
                                <% else %>
                                <td class="mycell col-md-1">
                                    <span class="label label-success">EXPIRED</span>
                                </td>
                                <%end%>
                                <% prices = {} %>
                                <% f.fuel_products.each do |product| %>
                                <% prices[product.fuel] = product.price %>
                                <%end%>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "87 RFG E10" %>
                                    $<%= prices["87 RFG E10"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "89 RFG E10" %>
                                    $<%= prices["89 RFG E10"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "91 RFG E10" %>
                                    $<%= prices["91 RFG E10"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "93 RFG E10" %>
                                    $<%= prices["93 RFG E10"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Low Sodium Diesel" %>
                                    $<%= prices["Low Sodium Diesel"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Dyed ULS Diesel" %>
                                    $<%= prices["Dyed ULS Diesel"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Undyed ULS Diesel" %>
                                    $<%= prices["Undyed ULS Diesel"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Jet Fuel" %>
                                    $<%= prices["Jet Fuel"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Heating Oil" %>
                                    $<%= prices["Heating Oil"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                                <td class="mycell col-md-1">
                                    <% if prices.include? "Kerosene" %>
                                    $<%= prices["Kerosene"]%>
                                    <%else%>
                                    <span class="label label-warning">Not set</span>
                                    <%end%>
                                </td>
                            </tr>
                            <% x = x + 1 %>
                            <%end%>
                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
